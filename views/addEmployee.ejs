<table border="1">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Salary</th>
    </tr>
    <% emp.forEach((emp)=>
        { %>
        <tr>
            <td>
                <%= emp.id %>
            </td>
            <td>
                <%= emp.name %>
            </td>
            <td>
                <%= emp.salary %>
            </td>
        </tr>
        <% }) %>
</table>